# Samadhan AI Frontend Environment Configuration
# Copy this file to .env and fill in your actual values

# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project_id.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project_id.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_messaging_sender_id
VITE_FIREBASE_APP_ID=your_app_id
VITE_FIREBASE_MEASUREMENT_ID=your_measurement_id

# Gemini AI Configuration
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Application Configuration
VITE_APP_NAME=Samadhan AI
VITE_APP_VERSION=1.0.0
VITE_APP_ENVIRONMENT=development

# Feature Flags
VITE_ENABLE_VOICE_CHAT=true
VITE_ENABLE_SPEECH_SYNTHESIS=true
VITE_ENABLE_FIREBASE_ANALYTICS=true

# Optional: Backend URL (if you have a backend later)
# VITE_BACKEND_URL=http://localhost:5000

# Optional: External Services
# VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
# VITE_WEATHER_API_KEY=your_weather_api_key

# Development Settings
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=info
